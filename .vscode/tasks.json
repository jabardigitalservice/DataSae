{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Generate Docs",
            "type": "docker-run",
            "dockerRun": {
                "image": "kgrv/pdoc3:snapshot",
                "containerName": "pdoc3",
                "remove": true,
                "customOptions": "-v ./datasae/:/workspace/datasae/:ro -v ./docs:/workspace/docs/",
                "command": "datasae"
            }
        }
    ]
}
